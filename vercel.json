{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "functions": {
    "src/server/index.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "dist/client/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "FRONTEND_URL": "https://context-bridge.vercel.app"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/src/server/index.ts"
    }
  ]
}